<?xml version="1.0" encoding="UTF-8"?>
<MOML>
    <THEMES>
        <THEME id = "style1" style="round" padding="10" margin = "5" />
    </THEMES>
    <UILAYOUT portrait="320,460" landscape="320,460">
        <WINDOW layout="0,0,320,460" defaultImg="#ffffff">
            <LABEL layout="10,10,auto,auto" text="Hello MOML" />
            <!--<WEBVIEW layout="10,60,300,300" src="http://naver.com" controls="none" />-->
            <CAMERA id="cam" layout="10,60,300,300" />
            <WINDOW layout="10, 380, 300, auto" align="linear">
                <STYLEBUTTON themId="style1" text="open" img="#0080ff" onClick="cam.open"/>
                <STYLEBUTTON themId="style1" text="close" img="lightgreen" onClick="cam.close"/>
                <STYLEBUTTON themId="style1" text="front" img="yellow" onClick="cam.type='front'"/>
                <STYLEBUTTON themId="style1" text="back" img="#00ff80" onClick="cam.type='back'"/>
                <STYLEBUTTON themId="style1" text="take" onClick="cam.takePicture"/>
        </WINDOW>
    </UILAYOUT>
</MOML>
